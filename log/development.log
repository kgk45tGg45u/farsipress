  [1m[36mSandbox Create (0.4ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "لیلا نقدی‌پری، سینماگر، در «زندان قرچک» و «بازداشت موقت» است"], ["content", "مجید برزگر، کارگردان و همسر لیلا نقدی‌پری که در جریان سالگرد کشته‌شدن مهسا (ژینا) امینی بازداشت شد، می‌گوید برای این سینماگر زن «حکم بازداشت موقت» صادر شده است.\n\nآقای برزگر بامداد دوشنبه با بیان اینکه خانم نقدی‌پری ظهر روز ۲۵ شهریور از خانه خارج شد، در اینستاگرام نوشت که همسرش شامگاه یکشنبه بعد از حدود ۳۰ ساعت بی‌خبری از زندان قرچک تماس گرفته و «قاطع و مسلط» گفته که حالش خوب است.\n\nلیلا نقدی‌پری، سینماگر و عضو هیئت مدیره انجمن طراحان فیلم سینمای ایران، سال گذشته هم به دلیل انتشار ویدئویی در حمایت از اعتراضات سراسری، چند بار احضار شده بود.\n\nاو سال گذشته و هنگام خروج از کشور برای حضور در یک کارگاه آموزشی سینمایی در فرودگاه خمینی ساعاتی بازداشت و بازجویی شد و پس از ضبط گذرنامه‌اش یک‌سال ممنوع‌الخروج شد.\n\n\n\nمجید برزگر که خود نیز سال گذشته به دلیل امضای بیانیه امضای بیانیه «تفنگت را زمین بگذار» به یک سال حبس و ممنوعیت از فعالیت‌های حرفه‌ای محکوم شده، در پست روز دوشنبه با اشاره به اینکه «تمام شهرِ تا بُنِ دندان نظامی و مسلح» را برای پیدا کردن همسرش جستجو کرده، گفت «جمعیتی یک‌ دو هزار نفری» از خانوادهٔ شهروندان بازداشت‌شده روز یکشنبه ۲۶ شهریور در برابر «ستاد فرماندهی امداد نیروی انتظامی» در محله افسریه حضور داشته‌اند.\n\nاین کارگردان با بیان اینکه هیچ‌یک از مسئولان در این مرکز انتظامی پاسخگو نبوده‌، از قول افراد تازه آزاد شده نوشت که «هزار نفر» از بازداشت‌شدگان در این مکان نگهداری می‌شدند.\n\n\n\nپیشتر کانال تلگرامی وحیدآنلاین با انتشار چند عکس از مقابل این مکان از انتقال همه افرادی که طی صبح و بعد از ظهر روز شنبه ۲۵ شهریور در حد فاصل میدان انقلاب تا میدان آزادی بازداشت شده‌اند، به بازداشتگاه این ستاد خبر داد.\n\nدر تصاویر منتشر شده از محوطه مقابل «ستاد فرماندهی امداد نیروی انتظامی» دیده‌ می‌شود که صدها تن از خانواده‌های بازداشت‌شدگان در حال انتظار برای روشن شدن وضعیت بستگان خود هستند و نیروی انتظامی نیز با افراد حاضر با خشونت برخورد می‌کند.\n\nدر این میان روزنامه شرق به استناد اظهارات مقام‌های رسمی نوشت که روز شنبه ۲۵ شهریور در سالگرد جان باختن مهسا امینی «دست‌کم ۲۷۰ تن در شهرهای مختلف به غیر از تهران» بازداشت شده‌اند و افزود که اما «آمار و ارقام غیررسمی» حکایت از بازداشت‌های بیشتری دارد.\n\nبا استفاده از گزارش شبکه‌های اجتماعی و رادیو فردا؛پ.م/پ.پ.\n"], ["source", "رادیو فردا"], ["created_at", "2023-09-18 14:58:03.829172"], ["updated_at", "2023-09-18 14:58:03.829172"]]
