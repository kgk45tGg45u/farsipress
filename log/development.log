  [1m[36mSandbox Create (0.3ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "سرنوشت صدها هزار دانش آموز هرمزگانی در گرو آمارهای غیر واقعی"], ["content", "<p>نائب رئیس شورای اسلامی شهر بندرعباس گفت: از استاندار که همیشه دغدغه ارتقا کیفیت آموزش را دارند و نمایندگان مجلس که نسبت به سرنوشت صدها هزار دانش آموز هرمزگانی مسئولند و باید زبان مردم در بیان این موضوعات و بازخواست آموزش و پرورش باشند، خواهشمندم هرچه سریع‌تر فکری به حال کیفیت آموزش باشند.</p> <p style=\"text-align: justify;\">به گزارش ایلنا از بندرعباس، یعقوب دبیری نژاد&nbsp;در نود و سومین جلسه رسمی شورای اسلامی شهر بندرعباس که با موضوع بازگشایی مدارس برگزار شد، با انتقاد از وضعیت آموزش در هرمزگان و تنگناهای مدیران مدارس در آستانه بازگشایی مدارس، اظهار کرد: تقویت آموزش و پرورش تضمین کننده تربیت نسل مدیران آینده استان است. در همین راستا شهرداری بندرعباس از ابتدای ١٤٠١ تا پایان مرداد ماه سال جاری بیش از ١٤ میلیارد تومان به صورت نقدی به حساب آموزش و پرورش واریز کرده است و طی چند ماه اخیر بیش از ٣ میلیارد و پانصد میلیون تومان به مدارس خدمات ارائه کرده است.</p> <p style=\"text-align: justify;\">وی افزود: در مقابل ما به عنوان نمایندگان مردم موظفیم نظارت کنیم که آیا این کمک&zwnj;ها در محل واقعی صرف توسعه و تجهیزات مدارس شده است یا نه؟ بنابراین توقع داریم اداره کل آموزش و پرورش ریز هزینه کرد این پول را به اعضای شورا مکاتبه کند.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:56:43.219438"], ["updated_at", "2023-09-18 14:56:43.219438"]]
