  [1m[36mSandbox Create (0.3ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "توضیحات رئیس کل دادگستری استان گلستان درباره آزادسازی جزیزه آشوراده"], ["content", "<p>رئیس کل دادگستری استان گلستان در نشست خبری خود در مورد آزادسازی جزیزه آشوراده توضیحاتی ارائه داد.</p> <p style=\"text-align: justify;\">به گزارش خبرنگار ایلنا، حیدری آسیابی در نشست خبری رئیس کل دادگستری استان گلستان درباره آزادسازی جزیزه آشوراده گفت: مشکلاتی به عنوان ابر چالش استان مطرح بود، در سفری که ۲۵ آذر سال ۱۴۰۰ رئیس قوه قضائیه به استان داشتند، مسائلی به سمع و نظر رئیس قوه قضائیه رسید و ایشان دستوراتی را دادند.</p> <p style=\"text-align: justify;\">رئیس کل دادستانی گلستان ادامه داد: مسائلی مانند پتروشیمی گلستان، گردشگری و ساخت و سازهای غیر مجاز در روستای آشوراده مطرح شد و در آن سفر اختیارات ویژه&zwnj;ای به دادگستری استان داده شود.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:58:05.794685"], ["updated_at", "2023-09-18 14:58:05.794685"]]
