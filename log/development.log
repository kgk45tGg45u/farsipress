  [1m[36mSandbox Create (1.4ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "حذف گزینه نان کنجددار از کارتخوان نانوایی‌های استان مرکزی "], ["content", "<p>مدیرکل دفتر هماهنگی امور اقتصادی استانداری مرکزی با بیان اینکه گزینه نان کنجددار از امروز (دوشنبه) از کارتخوان نانوایی‌های استان مرکزی حذف شد، گفت: در مرحله جدید اجرای طرح هوشمندسازی یارانه نان در استان مرکزی، محصولی تحت عنوان نان کنجددار در کارت‌خوان نانوایی‌ها وجود نخواهد داشت. </p> <p dir=\"RTL\" style=\"text-align: justify;\">به گزارش ایلنا، محمدرضا گودرزی، افزود: در این راستا کنجد و کیسه پلاستیکی به عنوان 2 محصول جداگانه در کارتخوان&zwnj;ها تعریف شده&zwnj;اند تا از یک سو عملکرد نانوا شفاف باشد و از سوی دیگر خطایی در اختصاص سهمیه آرد و پرداخت یارانه وجود نداشته باشد.</p> <p dir=\"RTL\" style=\"text-align: justify;\">وی ادامه داد: دولت به ازای هر نان 40 درصد یارانه نقدی به نانوا پرداخت می&zwnj;کند. بنابراین خریدار و نانوا باید دقت داشته باشند که در خرید نان صرفاً تعداد نان خریداری شده در کارتخوان ثبت شود.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:58:05.875678"], ["updated_at", "2023-09-18 14:58:05.875678"]]
