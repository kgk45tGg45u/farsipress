  [1m[36mSandbox Create (0.7ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at", "edited_title", "edited_content", "photo_url") VALUES ($1, $2, $3, $4, $5, $6, $7, $8) RETURNING "id"[0m  [["title", "ایجاد بانک جامع اطلاعات محرومیت کشور/ برگزاری رویداد روستایی کشور در مصلی تهران"], ["content", "<p>معاون توسعه روستایی و مناطق محروم کشور از برگزاری «روستا آباد» رویداد روستایی کشور از ۱۲ تا ۱۸ مهرماه در مصلی تهران خبر داد.</p> <p>به گزارش ایلنا، سید امیر حسین مدنی معاون توسعه روستایی و مناطق محروم کشور در نشستی با مدیران کل روستایی استانداری های سراسر کشور که امروز چهارشنبه در محل معاونت توسعه روستایی و مناطق محروم کشور برگزار شد، گفت: باتوجه به اینکه در دولت قبل رویکرد توجه به روستاها و مناطق محروم کشور ضعیف بود این معاونت نیز در عملکرد خود دچار مشکلات و ضعفهایی شده بود اما از آنجا که رویکرد دولت سیزدهم توجه به روستاها و مناطق محروم کشور است اتفاقات خوبی برای این معاونت رقم خورده که باعث توجه و اهتمام بیشتر به آبادانی روستاهای کشور شده است.</p> <p>مدنی تصریح کرد: پیش تر تصمیم گیری در امور روستا به صورت پراکنده در نهادهای متعددی انجام می&zwnj;شد که این موضوع در دولت سیزدهم با تشکیل شورای عالی جهاد سازندگی، امور عشایر و توسعه روستایی مرتفع شد و معاونت توسعه روستایی و مناطق محروم کشور محور تصمیم گیری ها برای امور روستا شده است.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-20 17:55:13.457150"], ["updated_at", "2023-09-20 17:55:13.457150"], ["edited_title", nil], ["edited_content", nil], ["photo_url", "https://cdn.ilna.ir/thumbnail/aTph6D0drqIO/YdHYY4gWkVztLEYWdaSeGA96qHB4I0VpTYn7J-mW3dR5Hc_1VoAc02R5PeisCjWL9ULcwlXt6ht7JMG7f2xXElgNtaCd5wQzXCnctAJbsO7SpDXtPsNSLA,,/Untitled.jpg"]]
