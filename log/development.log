  [1m[36mSandbox Create (0.2ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "سازمان ملل صدای ملت‌ها باشد نه صدای قدرت‌ها"], ["content", "<p>رئیس جمهور تاکید کرد: انتظار داریم سازمان ملل صدای ملت‌ها باشد نه صدای قدرت‌ها. ملت بزرگ ایران در حق‌خواهی، عدالت‌خواهی و مبارزه با تبعیض و بی‌عدالتی حرف‌های زیادی برای گفتن دارد. امیدوارم به عنوان نماینده ملت بزرگ ایران آنچه اقتضای این نمایندگی است را بیان کنم.</p> <p style=\"text-align: justify;\">به گزارش ایلنا، سید ابراهیم رئیسی&nbsp;در بدو ورود به نیویورک گفت: از فرصت استفاده می&zwnj;کنیم که صدای ملت ایران را به گوش جهانیان برسانیم؛ این صدا امروز رساتر از همیشه است چرا که سال گذشته پیروزی بزرگی را در مقابل جنگ شناختی و ترکیبی دشمن پشت سر گذاشتیم.</p> <p style=\"text-align: justify;\">وی افزود: سازمان ملل متحد و مجمع عمومی این سازمان دارای ظرفیتی است که می&zwnj;توان سخن مردم ایران را در آن بیان کرد و سیاست خارجی جمهوری اسلامی ایران را تبیین کرد.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:56:43.116850"], ["updated_at", "2023-09-18 14:56:43.116850"]]
