  [1m[36mSandbox Create (0.2ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "پرونده قضایی برای سقوط قطعات موشک در گرگان تشکیل شد"], ["content", "<p>رئیس کل دادگستری استان گلستان گفت: پس از سقوط قطعات موشک در چندین نقطه شهرگرگان پرونده قضایی در این مورد تشکیل شد و خوشتبخانه شهرگرگان در وضعیت عادی قرار دارد.</p> <p style=\"text-align: justify;\">به گزارش ایلنای گلستان، حیدر آسیابی اظهار کرد: طبق گفته&zwnj;های وزارت دفاع یکی از سامانه&zwnj;های موشکی دچار نقص فنی شد و با انحراف از مسیر اصلی متلاشی می&zwnj;شود و در مناطقی از شهر گرگان سقوط کرد.</p> <p style=\"text-align: justify;\">وی ادامه داد: امروز بعد از این حادثه ما در این مناطق حضور پیدا کردیم و به دادسرای نظامی استان گلستان&nbsp; ماموریت دادیم که پرونده قضایی در این رابطه تشکیل دهد. همه خسارت&zwnj;های که به مردم وارد شده شناسایی و بررسی شود.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:56:43.187606"], ["updated_at", "2023-09-18 14:56:43.187606"]]
