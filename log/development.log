  [1m[36mSandbox Create (0.3ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at", "edited_title", "edited_content") VALUES ($1, $2, $3, $4, $5, $6, $7) RETURNING "id"[0m  [["title", "امیدها به شاگردان عنایتی در هانگژو"], ["content", "<p>تیم ملی امید ایران در گام نخست نمایش خوبی مقابل عربستان داشتند.</p> <p style=\"text-align:justify\">به گزارش ایلنا، ایران در اولین بازی&zwnj;اش در مسابقات فوتبال بازی&zwnj;های آسیایی هانگژو نمایش نسبتاً خوبی داشت، ولی نتوانست دروازه عربستان را باز کند تا بازی با تساوی بدون گل به پایان برسد. تیم عنایتی در این بازی فرصت&zwnj;های بهتری از حریف داشت، ولی هیچ کدام از آنها وارد دروازه نشد تا سهم ایران فقط یک امتیاز باشد.</p> <p style=\"text-align:justify\">نیمی از بازیکنان اصلی تیم چهره&zwnj;هایی بودند که به تازگی در مسابقات مقدماتی المپیک در قالب تیم امید به میدان رفتند. سامان تورانیان، حسین گودرزی، امیر جعفری و محمدحسین اسلامی از مسافران تاشکند بودند که دیروز مقابل عربستان هم ثابت بازی کردند.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-20 15:37:06.610395"], ["updated_at", "2023-09-20 15:37:06.610395"], ["edited_title", nil], ["edited_content", nil]]
