  [1m[36mSandbox Create (0.5ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "ایران مصمم به توسعه روابط با کشورهای آمریکای جنوبی از جمله شیلی است"], ["content", "<p>وزیر امور خارجه تاکید کرد: دولت جمهوری اسلامی ایران مصمم به بسط سیاست خارجی متوازن و توسعه‌ی روابط با کشورهای آمریکای جنوبی از جمله شیلی است.</p> <p style=\"text-align: justify;\">به گزارش<a class=\"saba-backlink\" href=\"https://www.mehrnews.com/\">&nbsp;</a>ایلنا،&nbsp;حسین&nbsp;امیرعبداللهیان در حساب کاربری خود در شبکه اجتماعی ایکس&nbsp;نوشت: سالروز استقلال شیلی را به ملت، دولت و آقای آلبرتو ون&nbsp;کلاورن&nbsp;وزیر امور خارجه این کشور تبریک عرض می&zwnj;کنم.</p> <p style=\"text-align: justify;\">وی با بیان اینکه&nbsp;دولت جمهوری اسلامی ایران مصمم به بسط سیاست خارجی متوازن و توسعه&nbsp;روابط با کشورهای آمریکای جنوبی از جمله شیلی است، اظهار داشت:&nbsp;برای شیلی شادی و پیشرفت&zwnj;های روزافزون را آرزو دارم.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:56:43.085133"], ["updated_at", "2023-09-18 14:56:43.085133"]]
