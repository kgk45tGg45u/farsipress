  [1m[36mSandbox Create (0.3ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "تبادل زندانیان ایرانی و آمریکایی"], ["content", "هم‌زمان با انجام مبادله برخی از زندانیان ایرانی و آمریکایی، دارایی‌های بلوکه‌شده ایران به حساب ۶ بانک ایرانی در دوحه واریز شد. دو نفر از پنج زندانی آزادشده ایرانی وارد دوحه شدند.بر اساس توافق میان ایران و آمریکا بر سر آزادسازی حدود ۶ میلیارد دلار از دارایی&zwnj;هایی مسدود شده ایران در کره جنوبی، شهروندان آمریکایی زندانی در ایران با تعدادی از زندانیان ایرانی در آمریکا مبادله خواهند شد. شماری از جمهوری&zwnj;خواهان این اقدام دولت بایدن را &quot;پرداخت باج&quot; به حکومتی توصیف کرده&zwnj;اند که از سوی واشنگتن به عنوان حامی تروریسم شناخته می&zwnj;شود."], ["source", "دویچه وله"], ["created_at", "2023-09-18 14:58:04.279021"], ["updated_at", "2023-09-18 14:58:04.279021"]]
