  [1m[36mSandbox Create (0.6ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "روسیه با موشک و پهپادهای شاهد ایرانی بار دیگر زیرساخت بندری اوکراین را هدف قرار داد"], ["content", "اوکراین از حملات موشکی و پهپادی روسیه به تأسیسات کشاورزی این کشور در بنادر جنوبی اودسا در ساعات آغازین روز یکشنبه ۲۶ شهریورماه خبر داد.\n\nهم‌زمان روسیه نیز از حملات پهپادی اوکراین به مسکو و برخی مناطق دیگر کشور خبر داده است.\n\nاوکراین طی روزهای گذشته مجموعه حملاتی به اهدف نظامی در منطقه اشغالی کریمه و همچنین ناوگان دریایی روسیه در دریای سیاه انجام داده است.\n\nحملات پهپادی اوکراین در عمق خاک روسیه نیز طی روزهای گذشته افزایش یافته و شهردار مسکو می‌گوید در ساعات آغازین روز یکشنبه، دست‌کم دو پهپاد اوکراینی در آسمان این شهر ساقط شده است.\n\nبنابر اعلام نیروی هوایی ارتش اوکراین، روسیه بار دیگر با شش پهپاد ایرانی مدل شاهد و ۱۰ موشک بالدار، زیرساخت بندری اودسا را هدف قرار داده و ارتش اوکراین نیز با جنگنده‌ها، سامانه‌های ضدهوایی، گروه‌های سیار آتش‌نشانی و عوامل بازدارندۀ دیگر کوشیده است با حملات هوایی بامداد یکشنبه مقابله کند.\n\n\n\nحملات هوایی روسیه حدوداً یک روز پس از اعلام ورود دو کشتی باری اوکراینی به این بندر برای بارگیری غلات و ارسال آن از طریق یک کریدور موقت در دریای سیاه به کشورهای آفریقایی و آسیایی صورت گرفته است.\n\nدر همین زمینه، مقامات اوکراینی اعلام کرده بودند برای نخستین بار پس از خروج روسیه از توافق غلات دو کشتی باری با پرچم پالائو در بندر چرنومورسک پهلو گرفته‌اند و قرار است ۲۰ هزار تن گندم به مصر و اسرائیل حمل کنند.\n\nکی‌یف، در عین حال، برای صادرات محصولات کشاورزی خود از مسیرهای جایگزین با محدودیت‌های همسایگان اروپایی‌اش روبه‌روست. دولت اوکراین ماه گذشته از ایجاد یک «کریدور بشردوستانه» در دریای سیاه برای عبور کشتی‌های حامل غلات با دور زدن محاصره دریایی روسیه خبر داده بود.\n\nاما با آغاز تهاجم گستردۀ نظامی علیه اوکراین، روسیه شمال دریای سیاه را تحت کنترل خود درآورد و پس از خروج از توافق غلات که حدود یک سال ادامۀ صادرات محصولات کشاورزی اوکراین زیر نظر سازمان ملل را تضمین کرده بود، پیوسته بندرهای رود دانوب را که با مسیر اصلی جایگزین شد هدف حملات هوایی قرار می‌دهد.\n\nدر این میان، روز یکشنبه وزارت دفاع روسیه اعلام کرد که شش پهپاد اوکراینی را بر فراز منطقه کریمه که در سال ۲۰۱۴ توسط نیروهای روسی اشغال شده بود، ساقط کرده است. در این بیانیه به تلفات احتمالی حملات پهپادی اوکراین اشاره نشده است.\n\nشهردار مسکو نیز می‌گوید یک پهپاد اوکراینی بر فراز خیابان «ایسترا» و یکی دیگر در خیابان «رامنسکی» ساقط شده و هیچ تلفات یا خساراتی به بار نیاورده است.\n\nروسیه همچنین اعلام کرده است که در پی حملات پهپادی روز جاری اوکراین، دست‌کم ۳۰ پرواز از سه فرودگاه مسکو به تأخیر افتاد و شش پرواز نیز لغو شد.\n\nگزارش‌ها همچنین حاکی است که یک پهپاد اوکراینی در روز یکشنبه یک تانکر عظیم ذخیره سوخت در استان اوریول را هدف قرار داد و دود غلیظی از این منطقه، در جنوب روسیه، برخاست.\n\nآندری کلوچکوف، فرماندار اوریول گفته که این حمله تلفات انسانی نداشته است.\n\n\n\nپیشتر ولودیمیر زلنسکی، رئیس‌جمهور اوکراین، با انتشار ویدئویی در روز ۲۵ شهریورماه از متحدان خارجی خود به‌دلیل ادامه کمک‌های نظامی آن‌ها به کی‌یف برای مقابله با تهاجم نظامی روسیه قدردانی کرد و افزود: «طی هفته جاری ما پیشرفت چشمگیری در اجرایی کردن توافقات دفاعی موجود و بسته‌های کمکی دیگر داشتیم.»\n\nاو به‌ویژه از دانمارک به خاطر بسته جدید کمک نظامی که ۱۲-مین بسته تجهیزات تسلیحاتی، موشکی، سامانه‌های ضدهوایی و مهمات این کشور برای اوکراین است، قدردانی کرد.\n\nجیک سالیوان، مشاور امنیت ملی کاخ سفید،به‌تازگی تأیید کرد که جو بایدن، رئیس‌جمهور ایالات متحده، قرار است پنج‌شنبه آینده - سی‌ام شهریور - میزبان آقای زلنسکی در کاخ سفید باشد. این سومین دیدار رؤسای جمهور دو کشور در کاخ سفید خواهد بود.\n\nآقای زلنسکی و جو بایدن در نشست مجمع عمومی سازمان ملل سخنرانی خواهند کرد.\n\nانتظار می‌رود رئیس جمهور اوکراین از این فرصت برای دیدار با رهبران جهان و دریافت کمک‌های نظامی برای بازپس‌گیری اراضی اشغالی از نظامیان روسیه، استفاده کند.\n\nبا استفاده از گزارش‌های خبری رادیو اروپای آزاد/رادیو آزادی و رادیوفردا؛ د.خ/آ.ت.\n"], ["source", "Radio Farda"], ["created_at", "2023-09-17 17:54:26.181086"], ["updated_at", "2023-09-17 17:54:26.181086"]]
