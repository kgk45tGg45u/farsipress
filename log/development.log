  [1m[36mSandbox Create (1.4ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "ارتقای ۸۳ پله‌ای دانشگاه تهران در رتبه‌بندی عملکرد مقاله‌های علمی دانشگاه‌های جهان"], ["content", "<p>دانشگاه تهران با ۸۳ پله ارتقای جایگاه در ویرایش ۲۰۲۳ «رتبه‌بندی عملکرد مقاله‌های علمی دانشگاه‌های جهان» یا «رتبه‌بندی دانشگاه ملی تایوان» نسبت به ویرایش ۲۰۲۲ این نظام رتبه‌بندی، در جایگاه نخست ملی ایستاد. </p> <p>به گزارش ایلنا از روابط عمومی دانشگاه تهران، بر پایه گزارش سال ۲۰۲۳ میلادی &laquo;رتبه&zwnj;بندی عملکرد مقاله&zwnj;های علمی دانشگاه&zwnj;های جهان&raquo; یا &laquo;رتبه&zwnj;بندی دانشگاه ملی تایوان&raquo;، دانشگاه تهران در جایگاه نخست ملی و ۲۸۶ جهان ایستاده است. در سال گذشته دانشگاه تهران دومین دانشگاه ایرانی در این نظام رتبه&zwnj;بندی و رتبه جهانی آن ۳۶۹ بود.</p> <p>در نمودار یک، روند جایگاه دانشگاه تهران در این نظام رتبه&zwnj;بندی ترسیم شده است.&nbsp;</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:58:05.886945"], ["updated_at", "2023-09-18 14:58:05.886945"]]
