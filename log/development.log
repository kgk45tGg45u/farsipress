  [1m[36mSandbox Create (0.7ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "تکذیب پرداخت حق‌الزحمه به اعضای هیئت عالی مولدسازی از سوی رئیس دیوان عالی کشور"], ["content", "<p>رئیس دیوان عالی کشور اخبار منتشر شده در فضای مجازی در رابطه با پرداخت حق‌الزحمه به اعضای هیئت عالی مولدسازی را تکذیب کرد.</p> <p style=\"text-align:justify\">به گزارش ایلنا، حجت&zwnj;الاسلام منتظری رئیس دیوان عالی کشور امروز دوشنبه (۲۷ شهریور) در نشست شورای عالی قوه قضاییه با اشاره به مباحث مطرح شده در فضای مجازی تحت عنوان حق&zwnj;الزحمه&zwnj;های کلان اعضای هیئت عالی مولدسازی از محل فروش اموال و املاک، اظهار کرد: ضمن تکذیب جدی این موضوع، اعلام می&zwnj;کنم که تا به حال یک ریال هم به اعضای این هیات داده نشده و در آینده نیز داده نخواهد شد؛ حتی در کمیته فرعی مولدسازی نیز که یکی از نمایندگان قوه قضاییه در آن حضور دارد، به هیچ وجه قرار نیست مبلغی به اعضاء پرداخت شود.</p> <p style=\"text-align:justify\">&nbsp;</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:56:43.141726"], ["updated_at", "2023-09-18 14:56:43.141726"]]
