  [1m[36mSandbox Create (0.7ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at", "edited_title", "edited_content", "photo_url") VALUES ($1, $2, $3, $4, $5, $6, $7, $8) RETURNING "id"[0m  [["title", "ابرستاره مبهوت می‌شود"], ["content", "در ایستگاه فردا با زبان طنز به موضوعات و رویدادهای هفته می‌پردازیم.در این ایستگاه فردا ضمن پرداختن به حواشی و متن حضور رونالدو با تیم النصر در تهران، به حمله موشکی نظام به گرگان، عزم نظام برای برخورد با حجاب‌استایل‌ها، کارمزد سنگین اعضای هیات مولد‌سازی و حضور رئیسی در سامانه ضدفساد ریاست‌ جمهوری می‌پردازیم.\n\nایستگاه فردا روزهای چهارشنبه و پنج‌شنبه از ساعت هشت تا ۹ شب از رادیو فردا پخش می‌شود.\n\nبرای تماس با ما در تلگرام می‌توانید با FardaaStation@ در ارتباط باشید و یا از اپلیکیشن‌های iOS و اندروید ایستگاه فردا استفاده کنید.\n\n\n\n \n"], ["source", "رادیو فردا"], ["created_at", "2023-09-20 17:50:27.535457"], ["updated_at", "2023-09-20 17:50:27.535457"], ["edited_title", nil], ["edited_content", nil], ["photo_url", nil]]
