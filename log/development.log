  [1m[36mSandbox Create (0.2ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "کلیات لایحه اساسنامه سازمان توسعه سواحل مکران تصویب شد"], ["content", "<p>رئیس کمیسیون اقتصادی مجلس شورای اسلامی از تصویب کلیات لایحه اساسنامه سازمان توسعه سواحل مکران در جلسه این کمیسیون خبر داد و گفت: منطقه مکران یک منطقه بسیار بکر بوده و ظرفیت‌های سرمایه‌گذاری بسیار بالایی دارد.</p> <p style=\"text-align: justify;\">به گزارش ایلنا، محمدرضا پورابراهیمی در تشریح جزئیات جلسه عصر امروز کمیسیون اقتصادی مجلس شورای اسلامی، گفت: کمیسیون در جلسه عصر امروز خود کلیات دو لایحه اساسنامه سازمان توسعه سواحل مکران و لایحه موافقت&zwnj;نامه بین دولت جمهوری اسلامی ایران و دولت جمهوری مردمی بنگلادش به منظور حذف مالیات مضاعف در مورد مالیات&zwnj;های برآمد و جلوگیری از فرار و اجتناب مالیاتی را به تصویب رساند.</p> <p style=\"text-align: justify;\">وی در رابطه با لایحه موافقت&zwnj;نامه بین دولت جمهوری اسلامی ایران و دولت جمهوری مردم بنگلادش، تأکید کرد: تصویب این لایحه در زمینه سرمایه&zwnj;گذاری بین دو کشور اثرگذار است و باعث همکاری&zwnj;های اقتصادی بین دو کشور می&zwnj;شود.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-18 14:56:43.101218"], ["updated_at", "2023-09-18 14:56:43.101218"]]
