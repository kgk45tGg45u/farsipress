  [1m[36mSandbox Create (1.2ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at") VALUES ($1, $2, $3, $4, $5) RETURNING "id"[0m  [["title", "حقوق بازنشستگی ۹ میلیون تومان، هزینه زندگی ۳۰ میلیون تومان"], ["content", "اوضاع بازنشستگان کارگری ایران همواره اسفبارتر می‌شود. بر اساس آمار رسمی شمار قابل توجهی از آنها زیر خط فقر زندگی می‌کنند. تجمع هفتگی آنها در مقابل سازمان تامین اجتماعی هم تاکنون بی‌ثمر بوده است.بر اساس گزارشی از خبرگزاری ایلنا با عنوان بازنشستگان در تکاپوی نان، اوضاع بازنشستگان کارگری همواره بدتر می&zwnj;شود.\n\nدر این گزارش به نقل از یک کارگر بازنشسته آمده است، دریافتیِ او بعد از ۳۰ سال کار حدود ۹ میلیون تومان است. ایلنا ضمن یادآوری این که هزینه&zwnj; زندگی ۳۰ میلیون تومان است، می&zwnj;نویسد، این کارگر بازنشسته &laquo;شانس آورده&zwnj; که مستاجر نیست.&raquo;\n\nبیشتر بخوانید: تجمع بازنشستگان، فشار بر فعالان سیاسی و اخذ تعهد از دانشجویان\n\nدر ادامه گزارش آمده است که حدود ۵۰ درصد مستمری&zwnj;بگیران سازمان تامین اجتماعی فاقد خانه&zwnj; شخصی و مستاجر هستند.\n\nسال گذشته وزارت کار اعلام کرد که یک&zwnj;سوم جمعیت کشور زیر خط فقر هستند. شمار قابل توجهی از این افراد را بازنشستگان تشکیل می&zwnj;دهند. \n\nبیشتر بخوانید: تجمع سراسری بازنشستگان مخابرات، اعتصاب کارگران نفت و پتروشیمی\n\nبازنشستگان در یک سال گذشته هر یکشنبه مقابل سازمان تامین اجتماعی تجمع&zwnj; کرده&zwnj;اند تا این موضوع را به گوش مسئولان برسانند اما تاکنون کسی به مشکل آنها رسیدگی نکرده است. \n\nخبرگزاری ایلنا در ادامه گزارش خود نوشته است:&zwnj; &laquo;آنچه بازنشستگان می&zwnj;خواهند، افزایش مستمری&zwnj;ها براساس هزینه&zwnj;های زندگی و مطابق موازین قانونی&zwnj;ست. مواد ۱۱۱ و ۹۶ قانون تامین اجتماعی سال&zwnj;هاست اجرا نمی&zwnj;شود و امسال به بدترین شیوه&zwnj;ی ممکن نادیده گرفته شده است&raquo;.\n\nبه کانال دویچه وله فارسی در تلگرام بپیوندید\n<img width=\"1\" height=\"1\" alt=\"\" src=\"http://logc279.xiti.com/hit.xiti?s=531599&s2=22&p=per-rss-per_politik_volltext-5725-xml-mrss::%D8%AF%D9%88%DB%8C%DA%86%D9%87%20%D9%88%D9%84%D9%87%20%D9%81%D8%A7%D8%B1%D8%B3%DB%8C%3A%3A%D8%A7%DB%8C%D8%B1%D8%A7%D9%86::%D8%AD%D9%82%D9%88%D9%82%20%D8%A8%D8%A7%D8%B2%D9%86%D8%B4%D8%B3%D8%AA%DA%AF%DB%8C%20%DB%B9%20%D9%85%DB%8C%D9%84%DB%8C%D9%88%D9%86%20%D8%AA%D9%88%D9%85%D8%A7%D9%86%D8%8C%20%D9%87%D8%B2%DB%8C%D9%86%D9%87%20%D8%B2%D9%86%D8%AF%DA%AF%DB%8C%20%DB%B3%DB%B0%20%D9%85%DB%8C%D9%84%DB%8C%D9%88%D9%86%20%D8%AA%D9%88%D9%85%D8%A7%D9%86&di=&an=&ac=&x1=1&x2=22&x3=66834134&x4=9997&x5=%D8%AD%D9%82%D9%88%D9%82%20%D8%A8%D8%A7%D8%B2%D9%86%D8%B4%D8%B3%D8%AA%DA%AF%DB%8C%20%DB%B9%20%D9%85%DB%8C%D9%84%DB%8C%D9%88%D9%86%20%D8%AA%D9%88%D9%85%D8%A7%D9%86%D8%8C%20%D9%87%D8%B2%DB%8C%D9%86%D9%87%20%D8%B2%D9%86%D8%AF%DA%AF%DB%8C%20%DB%B3%DB%B0%20%D9%85%DB%8C%D9%84%DB%8C%D9%88%D9%86%20%D8%AA%D9%88%D9%85%D8%A7%D9%86&x6=1&x7=%2Ffa-ir%2F%D8%AD%D9%82%D9%88%D9%82-%D8%A8%D8%A7%D8%B2%D9%86%D8%B4%D8%B3%D8%AA%DA%AF%DB%8C-%DB%B9-%D9%85%DB%8C%D9%84%DB%8C%D9%88%D9%86-%D8%AA%D9%88%D9%85%D8%A7%D9%86%D8%8C-%D9%87%D8%B2%DB%8C%D9%86%D9%87-%D8%B2%D9%86%D8%AF%DA%AF%DB%8C-%DB%B3%DB%B0-%D9%85%DB%8C%D9%84%DB%8C%D9%88%D9%86-%D8%AA%D9%88%D9%85%D8%A7%D9%86%2Fa-66834134&x8=per-rss-per_politik_volltext-5725-xml-mrss&x9=20230916&x10=per-rss-per_politik_volltext-5725-xml-mrss::%D8%AF%D9%88%DB%8C%DA%86%D9%87%20%D9%88%D9%84%D9%87%20%D9%81%D8%A7%D8%B1%D8%B3%DB%8C%3A%3A%D8%A7%DB%8C%D8%B1%D8%A7%D9%86\" />"], ["source", "دویچه وله"], ["created_at", "2023-09-18 14:54:10.769101"], ["updated_at", "2023-09-18 14:54:10.769101"]]
