  [1m[36mSandbox Create (0.3ms)[0m  [1m[32mINSERT INTO "sandboxes" ("title", "content", "source", "created_at", "updated_at", "edited_title", "edited_content") VALUES ($1, $2, $3, $4, $5, $6, $7) RETURNING "id"[0m  [["title", "امشب؛ اولین نمایش شاگردان آنچلوتی برای کسب پانزدهمین قهرمانی لیگ قهرمانان"], ["content", "<p>رئال مادرید امشب در هفته اول لیگ قهرمانان اروپا به مصاف یونیون برلین خواهد رفت.</p> <p style=\"text-align:justify\">رئال مادرید در بازی خانگی امشب برابر اونیون برلین در هفته اول مرحله گروهی لیگ قهرمانان فصل 2024-2023 به دنبال ششمین برد متوالی خود است. در آخرین بازی لیگ، رئال 2 بر یک سوسیداد را برده و برلین با همین نتیجه مغلوب ولفسبورگ شده است.</p> <p style=\"text-align:justify\">رئال تا اینجای فصل تیم فوق&zwnj;العاده&zwnj;ای نبوده اما عالی نتیجه گرفته و با پنج برد و 15 امتیاز در صدر جدول لالیگا قرار دارد. تیم کارلو آنچلوتی در دو بازی اولش اتلتیک بیلبائو و آلمریا را با دو گل اختلاف شکست داد اما در سه بازی بعدی مقابل سلتاویگو، ختافه و سوسیداد با کمترین اختلاف به پیروزی رسیده است. این تیم که هفته آینده در سفری کوتاه به زمین اتلتیکومادرید می&zwnj;رود، به دنبال شروعی مقتدرانه در لیگ قهرمانان است؛ در گروهی که براگای پرتغال و ناپولی، قهرمان ایتالیا هم در آن حضور دارند.</p>"], ["source", "ایلنا"], ["created_at", "2023-09-20 15:37:17.127839"], ["updated_at", "2023-09-20 15:37:17.127839"], ["edited_title", nil], ["edited_content", nil]]
